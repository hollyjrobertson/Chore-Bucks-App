<!-- Sign Up Page -->
<% provide(:title, "Sign Up") %>
<div class="center jumbotron" id="signup">
  <h1>Sign up</h1>

  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= render 'form', user: @user %>
      <br>
      <p>Current User? <%= link_to "Login Now!", login_path, :class => "btn btn-success" %></p>
    </div>
  </div>
</div>
